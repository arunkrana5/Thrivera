@model List<Requirement.RequestList>
<div class="table-section">
    <div class="row">
        <div class="col-xl-12">
            <table id="example" class="table table-hover text-nowrap key-buttons table-responsive-sm">
                <thead>
                    <tr>
                        <th>SNo.</th>
                        
                        <th >Application Offered</th>
                        <th>Request No.</th>
                        <th>Request Date</th>
                        <th>Hired Type</th>
                        <th>Dealer Name</th>
                        <th>Region</th>
                        <th>State</th>
                        <th>Branch</th>
                    </tr>
                </thead>
                <tbody>
                    @{ int count = 0;}
                    @foreach (var item in Model)
                    {
                        count++;
                    <tr>
                        <td> @count</td>
                        <td class="text-green">
                            @item.ApplicationOffered
                        </td>
                        <td class="text-red">
                            <a title="View" href="@Url.Action("ApproveRequirementRequest", new { src = ClsCommon.Encrypt(ViewBag.MenuID + "*/Recurit/ApproveRequirementRequest*" + item.ReqID) })" data-toggle='tooltip'> @item.RequestNo </a>
                        </td>
                       
                        <td>@item.RequestDate</td>
                        <td>@item.HiredBy</td>
                        <td>@item.DealerName</td>
                        <td>@item.Region</td>
                        <td>@item.Region</td>
                        <td>@item.Branch</td>

                    </tr>
}

                </tbody>
            </table>
        </div>

    </div>
</div>